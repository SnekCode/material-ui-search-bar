{
  "name": "material-ui-search-bar",
  "version": "1.0.0",
  "description": "Material UI style search bar refactored from https://www.npmjs.com/package/material-ui-search-bar using typescript and tested with jest",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps",
    "prepare": "babel src -d lib --copy-files",
    "check-types": "tsc  --noEmit",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "publish-docs": "npm run styleguide:build && npm run gh-pages",
    "gh-pages": "./node_modules/.bin/gh-pages -d styleguide",
    "prettier": "prettier --write \"**/*.{json,md,js}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SnekCode/material-ui-search-bar.git"
  },
  "keywords": [
    "react",
    "material",
    "material-design",
    "material-ui",
    "search",
    "react-component"
  ],
  "author": "SnekCode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SnekCode/material-ui-search-bar/issues"
  },
  "homepage": "https://github.com/SnekCode/material-ui-search-bar#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.14.5",
    "@material-ui/core": ">4.0.0",
    "@material-ui/icons": ">4.0.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.0.5",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@material-ui/core": ">4.0.0",
    "@material-ui/icons": ">4.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "prop-types": "^15.5.8"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
